{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "guests": {
      "type": "integer"
    },
    "message": {
      "type": "richtext"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "time": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Approved"
      ],
      "default": "Pending",
      "required": false
    },
    "notification": {
      "type": "enumeration",
      "enum": [
        "Your Booking has been Appproved",
        "You have a Pending Booking"
      ],
      "default": "You have a Pending Booking"
    },
    "camel": {
      "type": "enumeration",
      "enum": [
        "Approved",
        "Pending"
      ]
    }
  }
}
